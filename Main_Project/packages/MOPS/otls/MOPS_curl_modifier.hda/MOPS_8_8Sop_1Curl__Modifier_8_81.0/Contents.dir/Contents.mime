--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/pointwrangle4.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	`chs(\"../Apply_Attributes2/group\")`	)
grouptype	[ 8	locks=0 ]	(	[ grouptype	0 ] 	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"int delprims = chi(\"delete_packed_prims\");
int maxiter = chi(\"Advection_Steps\");
float falloff = f@mops_falloff;
maxiter *= falloff;

float globalscale = chf(\"Overall_Scale\");
vector amp = chv(\"Amp\");
vector freq = chv(\"Frequency\");
vector offset = chv(\"Offset\");

`ifs(ch(\"../vexpression/do_vexpression\"), chs(\"../vexpression/snippet\"), \"\")`

vector oldpos = v@P;
vector move = {0,0,0};

int newprim = addprim(0, \"polyline\");

for(int i = 0; i < maxiter; i++){
    int newpt = addpoint(0, oldpos);
    addvertex(0, newprim, newpt);
    move = curlxnoise(v@P * freq + offset);
    v@P += move * amp * globalscale;
    setpointattrib(0, \"mops_N\", newpt, -normalize(move));
    setpointattrib(0, \"Cd\", newpt, v@Cd);
    
    float nfalloff = falloff;
    
    if(chi(\"scale_falloff\") == 1)
        nfalloff = falloff * (float(i) / float(maxiter));
    
    if(chi(\"overwrite_falloff\") == 1)
        nfalloff = float(i) / float(maxiter);
        
    setpointattrib(0, \"mops_falloff\", newpt, nfalloff);
    
    oldpos = v@P;
}

int newpt = addpoint(0, @ptnum);
addvertex(0, newprim, newpt);
setpointgroup(0, \"__mops_orig\", newpt, 1);
setpointgroup(0, \"__mops_orig\", @ptnum, 0);
setpointattrib(0, \"mops_N\", newpt, -normalize(move));

f@mops_falloff = falloff;
if(chi(\"overwrite_falloff\") == 1)
    f@mops_falloff = 1.0;

v@mops_N = -normalize(move);

if(delprims == 1)
    removepoint(0, @ptnum);"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
