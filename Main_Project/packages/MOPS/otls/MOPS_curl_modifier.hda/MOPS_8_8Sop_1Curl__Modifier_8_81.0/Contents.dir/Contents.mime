vex_selectiongroup	[ 0	locks=0 ]	(	""	)
folder1	[ 0	locks=0 ]	(	0	0	)
Overall_Scale	[ 0	locks=0 ]	(	[ Overall_Scale	1 ] 	)
delete_packed_prims	[ 0	locks=0 ]	(	[ delete_packed_prims	0 ] 	)
Advection_Steps	[ 0	locks=0 ]	(	[ Advection_Steps	50 ] 	)
scale_falloff	[ 0	locks=0 ]	(	[ scale_falloff	0 ] 	)
overwrite_falloff	[ 0	locks=0 ]	(	[ overwrite_falloff	0 ] 	)
Amp	[ 0	locks=0 ]	(	[ Ampx	1 ] 	[ Ampy	1 ] 	[ Ampz	1 ] 	)
