float weight = @attr;
if(!chi(\"do_falloff\")) {
    weight = 1;
}

vector pivot = chv(\"pivot\");
// we don't really want to lerp between \"pivot\" and \"new pivot\" in this case,
// because our pivot is in local space.

// vexpression
