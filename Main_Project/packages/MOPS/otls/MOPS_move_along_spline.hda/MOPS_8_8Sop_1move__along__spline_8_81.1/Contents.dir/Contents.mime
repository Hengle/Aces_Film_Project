Content-Disposition: attachment; filename="hdaroot/attach_to_curve_simple.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0	autoscope=1 ]	(	"// figure out how much to tweak falloff to allow for clean wrapping
float err = 1.0/((float)@numpt);
if(chi(\"wrap\")) {
    @__goalu = fit(@__goalu, 0, 1, 0, 1-err);
    @test = @__goalu;
}

// randomize goal
float randomAmount = ch(\"random_shift\");
int randomSeed = chi(\"random_seed\");
@__goalu += fit01(rand(i@id + (randomSeed*100000)), -randomAmount, randomAmount);

// determine travel and wrap targetu if wrap enabled
float goalu = ch(\"travel\") + @__goalu;
if(chi(\"wrap\")) {
    goalu %= 1.0;
} else {
    goalu = clamp(goalu, 0, 1);
}
goalu = 1.0 - goalu;
float offset_amount = ch(\"offset_amount\");


`ifs(ch(\"./do_vexpression\"), chs(\"./vexpression\"), \"\")`


// determine target position, orientation
vector targetP = primuv(1, \"P\", i@__curvenum, goalu);
vector tgt_binorm = primuv(1, \"mops_PT_binormal\", i@__curvenum, goalu);
vector tgt_tangent = primuv(1, \"mops_PT_tangent\", i@__curvenum, goalu);
vector4 rot = quaternion(maketransform(tgt_binorm, tgt_tangent));

// twisting
float twist_scale = ch(\"twist_scale\");
float twist_ramp = chramp(\"twist_ramp\", goalu);
vector4 twist = quaternion(radians(twist_scale) * twist_ramp, tgt_tangent);

rot = qmultiply(twist, rot);

// determine offset from curve based on the start of the curve.
vector originP = point(2, \"P\", @ptnum);
vector originTarget = primuv(1, \"P\", i@__curvenum, 0);
vector4 originO = primuv(1, \"orient\", i@__curvenum, 0);
vector offset = originP - originTarget;

// compute position. offset needs to be rotated around reference frame...?
offset = qrotate(rot, offset);
offset *= offset_amount;
@P = targetP + offset;

// compute orientation (can be disabled on Apply node)
p@orient = rot;
"	)
