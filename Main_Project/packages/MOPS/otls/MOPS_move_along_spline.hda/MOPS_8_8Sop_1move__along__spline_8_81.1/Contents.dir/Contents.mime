--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/solver1/d/s/pointwrangle1.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 0	locks=0 ]	(	""	)
grouptype	[ 0	locks=0 ]	(	"guess"	)
class	[ 0	locks=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0	autoscope=1 ]	(	"vector originP = point(3, \"P\", @ptnum);

// determine curve offset vector
vector originTarget = primuv(1, \"P\", 0, 0);
vector offset = originP - originTarget;

float offset_amount = ch(\"offset_amount\");
int wrap = chi(\"wrap\");

// random values
float randspeed = 1.0 + fit01(rand(i@id+chi(\"random_seed\")), -ch(\"random_speed\"), ch(\"random_speed\"));
float stepsize = chf(\"step_size\");
if(@Frame <= 1)
    stepsize = 0.0;
    
float goalu = f@goalu;


`ifs(ch(\"do_vexpression\"), chs(\"vexpression\"), \"\")`

    
// initial goal U on curve
goalu += (stepsize * randspeed * @mops_falloff);

if(wrap) {
    goalu %= 1.0;
} else {
    goalu = clamp(goalu, 0, 1);
}

float twist_scale = ch(\"twist\");



// twisting

float twist_ramp = chramp(\"twist_ramp\", goalu);
vector goalP = primuv(1, \"P\", i@__curvenum, goalu);
vector tgt_binorm = primuv(1, \"mops_PT_binormal\", i@__curvenum, goalu);
vector tgt_tangent = primuv(1, \"mops_PT_tangent\", i@__curvenum, goalu);
vector4 rot = quaternion(maketransform(tgt_tangent, tgt_binorm));
vector4 twist = quaternion(radians(twist_scale)*twist_ramp, tgt_tangent);
vector4 targetO = qmultiply(twist, rot);
vector4 prevO = primuv(1, \"orient\", i@__curvenum, 0);
vector rot_offset = qrotate( qmultiply( targetO, qinvert(prevO) ), offset);
v@P = goalP + (rot_offset * offset_amount);
//v@P = goalP;

vector4 origO = point(2, \"orient\", @ptnum);
float bias = chramp(\"blend_in_orient\", clamp(goalu, 0, 1));
// compute orientation (can be disabled on Apply node)
f@goalu = goalu;
p@orient = slerp(origO, targetO, bias);
"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"on"	)
bindings	[ 0	locks=0 ]	(	0	)
groupautobind	[ 0	locks=0 ]	(	"on"	)
groupbindings	[ 0	locks=0 ]	(	0	)
vex_cwdpath	[ 0	locks=0 ]	(	.	)
vex_outputmask	[ 0	locks=0 ]	(	*	)
vex_updatenmls	[ 0	locks=0 ]	(	"off"	)
vex_matchattrib	[ 0	locks=0 ]	(	id	)
vex_inplace	[ 0	locks=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0 ]	(	""	)
folder1	[ 0	locks=0 ]	(	0	0	)
step_size	[ 0	locks=0 ]	(	[ step_size	0.01 ] 	)
offset_amount	[ 0	locks=0 ]	(	[ offset_amount	0 ] 	)
random_speed	[ 0	locks=0 ]	(	[ random_speed	0 ] 	)
goalu	[ 0	locks=0 ]	(	0	)
twist	[ 0	locks=0 ]	(	[ twist	0 ] 	)
wrap	[ 0	locks=0 ]	(	[ wrap	1 ] 	)
