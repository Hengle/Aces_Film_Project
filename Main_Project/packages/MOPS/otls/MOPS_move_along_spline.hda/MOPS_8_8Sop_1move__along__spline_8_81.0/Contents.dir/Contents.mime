vex_matchattrib	[ 0	locks=0	autoscope=0 ]	(	id	)
vex_inplace	[ 0	locks=0	autoscope=0 ]	(	"off"	)
vex_selectiongroup	[ 0	locks=0	autoscope=0 ]	(	""	)
folder1	[ 0	locks=0	autoscope=0 ]	(	0	0	)
amp	[ 0	locks=0	autoscope=0 ]	(	[ amp	0 ] 	)
delprims	[ 0	locks=0	autoscope=0 ]	(	[ delprims	0 ] 	)
Advection_Steps	[ 0	locks=0	autoscope=0 ]	(	[ Advection_Steps	0 ] 	)
scale_falloff	[ 0	locks=0	autoscope=0 ]	(	[ scale_falloff	0 ] 	)
