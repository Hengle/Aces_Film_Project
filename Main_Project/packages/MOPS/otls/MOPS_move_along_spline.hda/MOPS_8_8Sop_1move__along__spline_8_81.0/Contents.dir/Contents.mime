--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/pointwrangle13.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0	autoscope=0 ]	(	0	0	)
group	[ 0	locks=0	autoscope=0 ]	(	""	)
grouptype	[ 0	locks=0	autoscope=0 ]	(	"guess"	)
class	[ 0	locks=0	autoscope=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0	autoscope=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0	autoscope=0 ]	(	1024	)
snippet	[ 0	locks=0	autoscope=0 ]	(	"int do_orient = chi(\"orient\");
float amp = chf(\"amp\");
amp *= f@mops_falloff;
float falloff = f@mops_falloff;

vector ctr = detail(0, \"__mops_center\");
vector oldpos = v@P - ctr;
vector restpos = v@P;

int npt = nearpoint(1, ctr);
float curveu = point(1, \"curveu\", npt);

vector newpos = primuv(1, \"P\", 0, curveu + amp);
matrix3 rot = primuv(1, \"__mops_rot\", 0, curveu + amp);

vector4 oldrot = p@orient;
vector4 newrot = quaternion(rot);

v@P = newpos;

if(do_orient){
    //v@P += oldpos * rot;
    v@P += oldpos;
    p@orient = newrot;
}
else {
    v@P += oldpos;
}

vector splinepos = primuv(1, \"P\", 0, curveu + amp);
splinepos += oldpos;
int newpt = addpoint(0, splinepos);
setpointattrib(0, \"mops_falloff\", newpt, falloff);
int newprim = addprim(0, \"polyline\", newpt);
setprimgroup(0, \"__mops_trails\", newprim, 1);

float stepsize = chf(\"trail_step_size\");
if(stepsize == 0)
    stepsize = 0.01;
for(float i = curveu + amp; i > curveu; i -= stepsize){
    splinepos = primuv(1, \"P\", 0, i);
    splinepos += oldpos;
    newpt = addpoint(0, splinepos);
    setpointattrib(0, \"mops_falloff\", newpt, falloff);
    addvertex(0, newprim, newpt);
}

splinepos = primuv(1, \"P\", 0, curveu);
splinepos += oldpos;
newpt = addpoint(0, splinepos);
setpointattrib(0, \"mops_falloff\", newpt, falloff);
addvertex(0, newprim, newpt);"	)
