{
version 0.8
folder0	[ 0	locks=0	autoscope=0 ]	(	0	0	)
group	[ 0	locks=0	autoscope=0 ]	(	""	)
grouptype	[ 0	locks=0	autoscope=0 ]	(	"guess"	)
class	[ 0	locks=0	autoscope=0 ]	(	"point"	)
vex_numcount	[ 0	locks=0	autoscope=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0	autoscope=0 ]	(	1024	)
snippet	[ 0	locks=0	autoscope=0 ]	(	"int delprims = chi(\"delprims\");
float amp = chf(\"amp\");
int maxiter = chi(\"Advection_Steps\");

float falloff = f@mops_falloff;
maxiter *= falloff;

vector oldpos = v@P;
vector move = {0,0,0};

for(int i = 0; i < maxiter; i++){
    int newpt = addpoint(0, oldpos);
    int npt = nearpoint(1, v@P);
    vector ntan = point(1, \"tangent\", npt);
    move = ntan;
    v@P += move * amp;
    setpointattrib(0, \"mops_N\", newpt, normalize(move));
    setpointattrib(0, \"Cd\", newpt, v@Cd);
    float nfalloff = falloff;
    
    if(chi(\"scale_falloff\") == 1)
        nfalloff = falloff * (float(i) / float(maxiter));
    
    if(chi(\"overwrite_falloff\") == 1)
        nfalloff = float(i) / float(maxiter);
        
    setpointattrib(0, \"mops_falloff\", newpt, nfalloff);
    oldpos = v@P;
}

f@mops_falloff = falloff;
if(chi(\"overwrite_falloff\") == 1)
    f@mops_falloff = 1.0;

v@N = normalize(move);
v@mops_N = v@N;

if(delprims == 1)
    removepoint(0, @ptnum);"	)
exportlist	[ 0	locks=0	autoscope=0 ]	(	*	)
