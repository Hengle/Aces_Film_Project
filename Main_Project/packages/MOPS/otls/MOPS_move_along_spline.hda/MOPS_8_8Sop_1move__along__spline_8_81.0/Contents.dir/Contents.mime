Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0	autoscope=0 ]	(	0	0	)
group	[ 0	locks=0	autoscope=0 ]	(	""	)
grouptype	[ 0	locks=0	autoscope=0 ]	(	"guess"	)
class	[ 0	locks=0	autoscope=0 ]	(	"primitive"	)
vex_numcount	[ 0	locks=0	autoscope=0 ]	(	10	)
vex_threadjobsize	[ 0	locks=0	autoscope=0 ]	(	1024	)
snippet	[ 0	locks=0	autoscope=0 ]	(	"int overwrite = chi(\"overwrite_falloff\");
int scale = chi(\"scale_falloff\");

int npts[] = primpoints(0, @primnum);
float curveu = 0;
float max = len(npts) -1;
float n = 0;
foreach(int npt; npts){
    curveu = n / max;
    if(overwrite){
        setpointattrib(0, \"mops_falloff\", npt, curveu);
    }
    else if (scale){
        float falloff = point(0, \"mops_falloff\", npt);
        falloff *= curveu;
        setpointattrib(0, \"mops_falloff\", npt, falloff);
    }
    n++;
}"	)
