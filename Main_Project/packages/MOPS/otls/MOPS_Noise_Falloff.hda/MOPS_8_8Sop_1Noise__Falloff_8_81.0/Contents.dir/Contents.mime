snippet	[ 0	locks=0	autoscope=0 ]	(	"matrix xform = primintrinsic(1, \"packedfulltransform\", 0);
vector4 pos = invert(xform) * @P;
vector4 freq = chp(\"freq\");
vector4 offset = chp(\"offset\");
float amp = chf(\"amp\");

